{
  "name": "marina-cli",
  "version": "2.1.1",
  "description": "Marina is a CLI tool to provide simple, shareable domains for your dev environment.",
  "author": "Shaun Harrison (@shnhrrsn)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shnhrrsn/marina-cli.git"
  },
  "keywords": [
    "reverse-proxy",
    "caddy",
    "ngrok",
    "dnsmasq",
    "docker"
